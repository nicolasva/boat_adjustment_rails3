%form#edit_adjustment_types
  %ul
    :each adjustmentType in adjustmentTypes
      %li
        = adjustmentType.name
        %ol{id: "adjustment_type_"+adjustmentType.id+"_"+App.set_indice_by_default()}
          :each adjustment in adjustmentType.adjustments
            %li
              %table{boarder: 1}
                %tr
                  %td
                    %input{type: 'text', name: "adjustment_type[adjustments_attributes]["+App.get_indice(1)+"][name]", value: adjustment.name}.
                  %td
                    %input{type: 'text', name: "adjustment_type[adjustments_attributes]["+App.get_indice(0)+"][value]", value: adjustment.value}
          
  %input{type: "submit", value: "Submit"}
